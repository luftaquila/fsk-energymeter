{
  "$schema": "https://schema.tauri.app/config/2.0.0",
  "productName": "fsk-energymeter",
  "version": "0.1.0",
  "identifier": "luftaquila.fsk-energymeter",
  "build": {
    "frontendDist": ["../../web", "../../web/node_modules"]
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "fsk-energymeter",
        "width": 1100,
        "height": 1200
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
